    {
      "path": "src\\Template.Persistence\\Template.Persistence.csproj"
    },
    {
      "path": "src\\Template.Infrastructure\\Template.Infrastructure.csproj"
    },

    {
      "path": "test\\API.IntegrationTests\\API.IntegrationTests.csproj"
    },
    {
      "path": "test\\Application.UnitTests\\Application.UnitTests.csproj"
    },
    {
      "path": "test\\Persistence.IntegrationTests\\Persistence.IntegrationTests.csproj"
    },
    {
      "path": "test\\UnitTests.Utils\\UnitTests.Utils.csproj"
    }

